set(PLUGIN_TRACECONTROL_AUTOSTART true CACHE BOOL "Automatically start TraceControl plugin")
set(PLUGIN_TRACECONTROL_CONSOLE false CACHE BOOL "Console log enabled")
set(PLUGIN_TRACECONTROL_SYSLOG true CACHE BOOL "SysLog enabled")
set(PLUGIN_TRACECONTROL_ABBREVIATED true CACHE BOOL "Log Abbreviation enabled")
set(PLUGIN_TRACECONTROL_REMOTE false CACHE BOOL "Remote binding details enabled")
set(PLUGIN_TRACECONTROL_PORT 0 CACHE STRING "PORT address")
set(PLUGIN_TRACECONTROL_BINDING "0.0.0.0" CACHE STRING "Binding IP Address")

set (autostart ${PLUGIN_TRACECONTROL_AUTOSTART})
map()
  kv(console ${PLUGIN_TRACECONTROL_CONSOLE})
  kv(syslog ${PLUGIN_TRACECONTROL_SYSLOG})
  kv(abbreviated ${PLUGIN_TRACECONTROL_ABBREVIATED})

  if (PLUGIN_TRACECONTROL_REMOTE)
  key(remote)
  map()
    kv(port ${PLUGIN_TRACECONTROL_PORT})
    kv(binding ${PLUGIN_TRACECONTROL_BINDING})
  end()
  endif()
end()
ans(configuration)
